<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ie.icon.domain.sale">
	<class name="RefundItemDiscount" table="REFUND_ITEM_DSCNT">
		<id name="objectId" column="REFUND_ITEM_DSCNT_OID" type="long">
			<generator class="sequence">
				<param name="sequence">REFUND_ITEM_DSCNT_SEQ</param>
			</generator>
		</id>
		<many-to-one name="refundItem" column="REFUND_ITEM_OID" lazy="false" not-null="true"/>
		<many-to-one name="discountConditionType" column="DSCNT_COND_TYP_ID" lazy="false" not-null="true"/>
		<property name="discountValue" column="DSCNT_VAL" type="big_decimal"/>
		<property name="discountPerUnit" column="DSCNT_PER_UNIT" type="big_decimal"/>
		<property name="discountAmount" column="DSCNT_AMT" type="big_decimal"/>
		<property name="remark" column="REMARK" type="string"/>
		<property name="isAutoGenerated" column="IS_AUTO_GEN" type="yes_no"/>
		<property name="authorizeUserId" column="AUTH_USER_ID" type="string"/>
	</class>
</hibernate-mapping>
