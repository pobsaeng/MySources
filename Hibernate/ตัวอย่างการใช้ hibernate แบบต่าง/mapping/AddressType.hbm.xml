<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ie.icon.domain.customer">
	<class name="AddressType" table="ADDR_TYP">
		<id name="objectId" column="ADDR_TYP_OID" type="long">
			<generator class="sequence">
				<param name="sequence">ADDR_TYP_SEQ</param>
			</generator>
		</id>
		<property name="addressTypeId" column="ADDR_TYP_ID" type="string"/>
		<property name="name" column="ADDR_TYP_NM" type="string"/>
	</class>
</hibernate-mapping>
