<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ie.icon.domain.so">
	<class name="StoreDeliveryArea" table="STORE_DELI_AREA">
		<id name="storeDeliveryAreaId" column="STORE_DELI_AREA_ID" type="string"/>
		<property name="description" column="STORE_DELI_AREA_DSC" type="string"/>
		<many-to-one name="store" column="STORE_ID" lazy="false" not-null="true"/>
	</class>
</hibernate-mapping>
