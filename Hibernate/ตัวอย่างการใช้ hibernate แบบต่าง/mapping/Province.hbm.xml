<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ie.icon.domain">
	<class name="Province" table="PROVINCE">
		<id name="objectId" column="PROVINCE_OID" type="long">
			<generator class="sequence">
				<param name="sequence">PROVINCE_SEQ</param>
			</generator>
		</id>
		<property name="nameThai" column="NM_THAI" type="string"/>
		<property name="nameEng" column="NM_ENG" type="string"/>
		<property name="lastUpdateDate" column="LAST_UPD_DTTM" type="timestamp"/>
		<property name="lastPublishedDateTime" column="LAST_PUB_DTTM" type="timestamp"/>
		<property name="referencePublishId" column="REF_PUB_ID" type="string"/> 
	</class>
</hibernate-mapping>
