<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ie.icon.domain.so">
	<class name="UserPc" table="USER_PC">
	    <id name="objectId" column="USER_PC_OID" type="long"></id> 
		<property name="userPcId" column="USER_PC_ID" type="string"/>
		<property name="userPcFirstNm" column="USER_PC_FIRST_NM" type="string"/>
		<property name="userPcLastNm" column="USER_PC_LAST_NM" type="string"/>
		<property name="createDateTime" column="CREATE_DTTM" type="timestamp"/>
		<property name="lastUpdateDate" column="LAST_UPD_DTTM" type="timestamp"/>
		<property name="lastPublishedDateTime" column="LAST_PUB_DTTM" type="timestamp"/>
		<property name="referencePublishId" column="REF_PUB_ID" type="string"/>
	</class>
</hibernate-mapping>



