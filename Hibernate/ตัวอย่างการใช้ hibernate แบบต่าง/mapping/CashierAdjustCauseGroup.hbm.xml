<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ie.icon.domain.cashier">
	<class name="CashierAdjustCauseGroup" table="CSH_ADJ_CAUSE_GRP">
	
		<id name="cashierAdjustCauseGroup" column="CSH_ADJ_CAUSE_GRP_ID" type="string" />
		<property name="cashierGroupDesc" column="CAUSE_GRP_DESC" type="string"/>	
		<property name="cashierGroupSeq" column="CAUSE_GRP_SEQ" type="string"/>	
