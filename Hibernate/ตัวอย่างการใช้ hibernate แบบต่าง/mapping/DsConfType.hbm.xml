<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ie.icon.domain.so">
	<class name="DsConfType" table="M_DS_CONFTYPE">
		<id name="confirmTypeId" column="CONF_TYP_NO" type="string"/>
		<property name="confirmTypeDesc" column="CON_TYP_DESC" type="string"/>
		<property name="isActive" column="IS_ACTIVE" type="string"/>
		<property name="createDate" column="CRDATE" type="timestamp"/>
		<property name="lastUpdateDate" column="LAST_UPD_DTTM" type="timestamp"/>
		<property name="lastPublishedDateTime" column="LAST_PUB_DTTM" type="timestamp"/>
		<property name="referencePublishId" column="REF_PUB_ID" type="string"/>
		<property name="seqNo" column="SEQ_NO" type="integer"/>    
	</class>
</hibernate-mapping>
